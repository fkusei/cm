[Unit]
Description = voctomix2 {{ item }}-to-framebuffer playout
After = voctomix2-voctocore.service
Requires = voctomix2-voctocore.service

[Service]
Type = simple
ExecStart = /opt/voctomix2/scripts/{{ item }}-to-framebuffer-playout.sh
Restart = always
RestartSec = 1s
StartLimitInterval = 0

[Install]
WantedBy = voctomix2-voctocore.service
